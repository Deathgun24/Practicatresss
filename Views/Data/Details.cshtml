@model IEnumerable<Practicatresss.Integration.data.Datos>

@{
    ViewData["Title"] = "DETALLES";
}
<div class="navi"></div>
<div class="navi"></div>
<div class="container">

    <h1>LISTA </h1>

    <table class=" " style="width: 100px;">
        <thead>
            <tr>
                <th>
                    Id Usuario
                </th>
                <th>
                    Primer nombre
                </th>
                <th>
                    Correo
                </th>
                <th>
                    Apellido
                </th>
                <th>
                    Avatar
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr class="table-row">
                    <td>
                        @Html.DisplayFor(modelItem => item.id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.first_name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.last_name)
                    </td>
                    <td>
                        <img src="@Html.DisplayFor(modelItem => item.avatar)" class="d-block image-fit ">
                    </td>
                    <td>

                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>